{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "": "./src",
          "@assets": "./src/assets",
          "@constants": "./src/constants",
          "@model": "./src/model",
          "@styles": "./src/styles",
          "@utils": "./src/utils",
          "@views": "./src/views"
        }
      }
    ]
  ]
}
